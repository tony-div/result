{
  "name": "result2",
  "version": "1.0.1",
  "description": "rust inspired easy, and efficient error handling in js",
  "files": [
    "./dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build",
    "docs": "rimraf ./docs && typedoc --entryPointStrategy Expand src --out docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tony-div/result"
  },
  "homepage": "https://tony-div.github.io/result/",
  "keywords": [
    "rust",
    "error",
    "result",
    "errors",
    "error-handling"
  ],
  "author": "Tony George",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.11",
    "typescript": "^5.9.2"
  }
}
